{
  "name": "figma-ds-builder",
  "version": "1.0.0",
  "description": "Your Figma plugin",
  "main": "code.js",
  "scripts": {
    "export-css": "jiti ./src/cli/export-css.ts",
    "export-json": "jiti ./src/cli/export-json.ts",
    "build-tokens": "grunt default",
    "watch": "npx webpack --mode=development --target=web --watch",
    "make": "npx webpack --mode=development --target=web",
    "make-cli": "npx webpack --mode=production --target=node",
    "build": "tsc -p tsconfig.json",
    "pack-cli": "npm pack ./cli --pack-destination ~",
    "pack-tw": "npm pack ./tw --pack-destination ~"
  },
  "author": "",
  "license": "",
  "devDependencies": {
    "@figma-plugin/helpers": "^0.15.2",
    "@figma/eslint-plugin-figma-plugins": "^0.14.0",
    "@figma/plugin-typings": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "css-loader": "^6.6.0",
    "eslint": "^8.57.0",
    "glob": "^10.3.10",
    "html-webpack-inline-source-plugin": "^1.0.0-beta.2",
    "html-webpack-plugin": "^5.5.3",
    "jiti": "^1.21.0",
    "mini-css-extract-plugin": "^2.7.6",
    "mini-svg-data-uri": "^1.4.4",
    "postcss": "^8.4.25",
    "postcss-loader": "^7.3.3",
    "postcss-nesting": "^12.0.1",
    "postcss-preset-env": "^9.0.0",
    "source-foundation-cli": "^1.0.6",
    "source-tailwindcss": "^1.0.5",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.5",
    "ts-loader": "^9.2.6",
    "typescript": "^5.3.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  }
}
